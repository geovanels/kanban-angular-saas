{
  "version": 2,
  "env": {
    "NODE_VERSION": "22"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/kanban-angular",
        "nodeVersion": "22.x"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}