{
  "name": "@ngx-formly/core",
  "version": "7.0.0",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/forms": ">=13.2.0",
    "rxjs": "^6.5.3 || ^7.0.0"
  },
  "module": "fesm2022/ngx-formly-core.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ngx-formly-core.mjs",
      "esm": "./esm2022/ngx-formly-core.mjs",
      "default": "./fesm2022/ngx-formly-core.mjs"
    },
    "./json-schema": {
      "types": "./json-schema/index.d.ts",
      "esm2022": "./esm2022/json-schema/ngx-formly-core-json-schema.mjs",
      "esm": "./esm2022/json-schema/ngx-formly-core-json-schema.mjs",
      "default": "./fesm2022/ngx-formly-core-json-schema.mjs"
    },
    "./preset": {
      "types": "./preset/index.d.ts",
      "esm2022": "./esm2022/preset/ngx-formly-core-preset.mjs",
      "esm": "./esm2022/preset/ngx-formly-core-preset.mjs",
      "default": "./fesm2022/ngx-formly-core-preset.mjs"
    },
    "./select": {
      "types": "./select/index.d.ts",
